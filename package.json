{
  "name": "rescript-docgen",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build:all": "bun run --filter '@rescript-docgen/*' --filter '@examples/*' res:build",
    "test:all": "bun run --filter '@rescript-docgen/*' test"
  },
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@rescript/core": "1.6.1",
    "@rescript/tools": "^0.6.6",
    "rescript": "11.1.4",
    "rescript-bun": "^0.5.0",
    "sury": "^10.0.4"
  }
}